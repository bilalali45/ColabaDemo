<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/filledTextInputLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:boxStrokeWidth="1dp"
        app:errorEnabled="true"
        android:defaultFocusHighlightEnabled="false"
        app:boxCollapsedPaddingTop="1dp"
        app:boxStrokeColor="@color/test_selector"
        app:boxStrokeWidthFocused="1dp"
        app:boxStrokeErrorColor="@color/colaba_red_color"
        app:errorTextAppearance="@style/error_appearance"
        app:errorIconDrawable="@null"
        >

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/relationshipEditText"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="0dp"
            android:maxLines="1"
            android:imeOptions="actionNext"
            android:singleLine="true"
            android:lines="1"
            android:textSize="15sp"
            android:fontFamily="@font/rubik_regular"
            android:hint="@string/mailing_address"

            android:textColor="@color/grey_color_one"

            />

    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/filledTextInputLayout2"
        android:layout_marginStart="32dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/filledTextInputLayout"


        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:boxStrokeWidth="1dp"
        app:errorEnabled="true"
        android:defaultFocusHighlightEnabled="false"
        app:boxCollapsedPaddingTop="1dp"
        app:boxStrokeColor="@color/test_selector"
        app:boxStrokeWidthFocused="1dp"
        app:boxStrokeErrorColor="@color/colaba_red_color"
        app:errorTextAppearance="@style/error_appearance"
        app:errorIconDrawable="@null"
        >

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/filledTextField2"
            android:hint="@string/mailing_address"



            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:paddingStart="0dp"
            android:maxLines="1"
            android:imeOptions="actionNext"
            android:singleLine="true"
            android:lines="1"
            android:textSize="15sp"
            android:fontFamily="@font/rubik_regular"
            android:textColor="@color/grey_color_one" />

    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/layout_first_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:layout_weight="1"
        android:defaultFocusHighlightEnabled="false"
        android:fontFamily="@font/rubik_light"
        android:hint="@string/first_name"
        android:textColorHint="@color/primary_info_label_color"

        app:boxCollapsedPaddingTop="1dp"
        app:boxStrokeColor="@color/primary_info_line_color"
        app:boxStrokeErrorColor="@color/colaba_red_color"
        app:boxStrokeWidth="1dp"
        app:boxStrokeWidthFocused="1dp"
        app:errorEnabled="true"

        app:errorIconDrawable="@null"
        app:errorTextAppearance="@style/error_appearance"
        app:helperTextEnabled="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/filledTextInputLayout">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/ed_first_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
            android:ems="10"
            android:fontFamily="@font/rubik_medium"
            android:imeOptions="actionDone"
            android:paddingStart="0dp"

            android:singleLine="true"
            android:textColor="@color/grey_color_one"
            android:textSize="@dimen/ed_txt_size_primary_info" />
    </com.google.android.material.textfield.TextInputLayout>


    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/layout_home_num"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/rubik_light"
        android:hint="@string/home_number"
        android:textColorHint="@color/primary_info_label_color"
        app:boxCollapsedPaddingTop="1dp"
        app:boxStrokeColor="@color/primary_info_line_color"
        app:boxStrokeWidth="0.5dp"
        app:boxStrokeWidthFocused="1dp"
        app:helperTextEnabled="true"
        app:helperTextTextAppearance="@style/inputError"
        app:helperTextTextColor="@color/colaba_red_color"
        app:hintTextAppearance="@style/TextAppearence.App.TextInputLayout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/ed_home_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:digits="1234567890-()"
            android:ems="10"
            android:fontFamily="@font/rubik_regular"
            android:imeOptions="actionDone"

            android:inputType="number"
            android:maxLength="14"
            android:paddingStart="0dp"
            android:singleLine="true"
            android:textColor="@color/grey_color_one"
            android:textSize="@dimen/ed_txt_size_primary_info" />

    </com.google.android.material.textfield.TextInputLayout>

