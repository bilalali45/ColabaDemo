<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ColabaDemo" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colaba_blue_color</item>
        <item name="colorPrimaryVariant">@color/colaba_blue_color</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colaba_blue_color</item>
        <item name="colorSecondaryVariant">@color/colaba_blue_color</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>


        <item name="android:fontFamily">@font/rubik_regular</item>
        <item name="fontFamily">@font/rubik_regular</item>


        <!-- Customize your theme here.
                <item name="android:fontFamily">@font/dancing_font</item>
                <item name="fontFamily">@font/dancing_font</item>
          -->
        </style>


    <style name="MyButtonStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>


