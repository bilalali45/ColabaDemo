    "universal-cookie": "^4.0.3"
  },
  "scripts": {
    "verify-installation": "node scripts/verify-install.js && node scripts/copy-pspdfkit-files.js",
    "start": "set PORT=3001 && node scripts/verify-install.js && node scripts/copy-pspdfkit-files.js && react-scripts start",
    "build": "node scripts/verify-install.js && node scripts/copy-pspdfkit-files.js && react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --verbose --silent",
    "test-dev": "react-scripts test --env=jest-environment-jsdom-sixteen --verbose",
    "test-coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --verbose --silent --coverage . --coverageDirectory='coverage'",
    "test-debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/services/test_helpers/*.{ts,tsx}",
      "!src/store/actions/*.{ts,tsx}",
      "!src/utils/image_assets/*.{ts,tsx}",
      "!src/utils/ParamsService.ts",
      "!src/store/endpoints/*.{ts,tsx}",
      "!src/test_utilities/*.{ts, tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts", 
      "!src/shared/Components/DocumentView/DocumentView.tsx"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.157",
    "@types/react-router-dom": "^5.1.5",
    "@types/universal-cookie": "^2.2.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "ncp": "^2.0.0"
  }
}