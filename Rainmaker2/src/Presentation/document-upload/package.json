    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.39",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "apollo-cache-inmemory": "^1.6.6",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "global": "^4.4.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "logrocket": "^1.0.14",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "print-js": "^1.0.63",
    "pspdfkit": "^2020.6.1",
    "rainsoft-js": "^1.0.49",
    "rainsoft-rc": "^1.0.23",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-file-viewer": "^1.2.1",
    "react-idle-timer": "^4.2.12",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-zoom-pan-pinch": "^1.6.1",
    "serve": "^11.3.2",
