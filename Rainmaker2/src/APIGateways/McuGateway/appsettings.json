{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*",
  "KeyStore": {
    "Url": "http://localhost:7072"
  },
  "LoggingMiddleware": {
    "RequestSize": 4, // in kb
    "ResponseSize": 4 // in kb
  },
  "Redis": {
    "ConnectionString": "127.0.0.1:6379,defaultDatabase=1"
  },
  "AllowedOrigins": [ "http://alpha.rainsoftfn.com", "http://alphatx.rainsoftfn.com", "https://alpha.rainsoftfn.com", "https://alphatx.rainsoftfn.com" ]
}